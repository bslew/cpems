# - Config file for the cpems package
# It defines the following variables
#  cpems_INCLUDE_DIRS - include directories for cpems
#  cpems_LIBRARIES    - libraries to link against
#  cpems_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(cpems_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(cpems_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET cpems AND NOT cpems_BINARY_DIR)
  include("${cpems_CMAKE_DIR}/cpemsTargets.cmake")
endif()

# These are IMPORTED targets created by cpemsTargets.cmake
set(cpems_LIBRARIES cpems)
#set(cpems_EXECUTABLE ${EXECUTABLES})
